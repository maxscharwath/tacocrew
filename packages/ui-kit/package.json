{
  "name": "@tacobot/ui-kit",
  "version": "1.0.0",
  "description": "Shared UI component library for TacoBot",
  "type": "module",
  "main": "./src/index.ts",
  "types": "./src/index.ts",
  "exports": {
    ".": {
      "development": "./src/index.ts",
      "types": "./src/index.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "esbuild src/index.ts --bundle --platform=browser --target=esnext --format=esm --outfile=dist/index.js --packages=external && tsc",
    "check": "tsc --noEmit",
    "dev": "tsc --watch"
  },
  "dependencies": {
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "class-variance-authority": "^0.7.1",
    "country-flag-icons": "^1.6.4",
    "libphonenumber-js": "^1.12.29",
    "sonner": "^1.7.1",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "esbuild": "^0.27.0",
    "typescript": "^5.9.3"
  },
  "peerDependencies": {
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-i18next": "^16.3.5"
  }
}

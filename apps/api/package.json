{
  "name": "@tacobot/api",
  "private": true,
  "version": "0.2512.1",
  "type": "module",
  "description": "Modern TypeScript API wrapper for tacos ordering system with Web API integration",
  "main": "dist/index.js",
  "exports": {
    ".": "./src/index.ts"
  },
  "scripts": {
    "dev": "bun --watch src/index.ts",
    "build": "bun build.ts",
    "start": "bun dist/index.js",
    "check": "tsc --noEmit --pretty",
    "prisma:generate": "bunx prisma generate",
    "prisma:migrate": "bunx prisma migrate dev",
    "prisma:migrate:deploy": "bunx prisma migrate deploy",
    "prisma:db:push": "bunx prisma db push --accept-data-loss",
    "prisma:studio": "bunx prisma studio",
    "test": "bun test",
    "test:watch": "bun test --watch"
  },
  "keywords": ["tacos", "api", "typescript", "rest-api"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@better-auth/passkey": "^1.4.5",
    "@hono/node-server": "^1.19.6",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.5",
    "@prisma/adapter-pg": "^7.0.1",
    "@prisma/client": "^7.0.1",
    "@tacobot/gigatacos-client": "workspace:*",
    "axios": "^1.13.2",
    "better-auth": "^1.3.34",
    "bs58": "^6.0.0",
    "cheerio": "^1.1.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "hono": "^4.10.6",
    "i18next": "^25.6.3",
    "i18next-fs-backend": "^2.6.1",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.16.3",
    "prisma": "^7.0.1",
    "reflect-metadata": "^0.2.2",
    "sharp": "^0.34.5",
    "tslib": "^2.8.1",
    "tsyringe": "^4.10.0",
    "uuid": "^13.0.0",
    "web-push": "^3.6.7",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.13"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/glob": "^9.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.1",
    "@types/pg": "^8.15.6",
    "glob": "^13.0.0",
    "typescript": "^5.9.3"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0",
    "bun": ">=1.2.0"
  }
}

{
  "name": "@tacobot/api",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "description": "Modern TypeScript API wrapper for tacos ordering system with Web API integration",
  "main": "dist/index.js",
  "exports": {
    ".": "./dist/index.js"
  },
  "scripts": {
    "dev": "tsx watch src/index.ts",
    "build": "esbuild src/index.ts --bundle --platform=node --target=esnext --format=esm --outfile=dist/index.js --packages=external",
    "check": "tsc --noEmit --pretty",
    "prisma:generate": "prisma generate",
    "prisma:migrate": "prisma migrate dev",
    "prisma:migrate:deploy": "prisma migrate deploy",
    "prisma:db:push": "prisma db push --accept-data-loss",
    "prisma:studio": "prisma studio",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui"
  },
  "keywords": ["tacos", "api", "typescript", "rest-api"],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "@hono/node-server": "^1.19.6",
    "@hono/swagger-ui": "^0.5.2",
    "@hono/zod-openapi": "^1.1.4",
    "@prisma/client": "^6.19.0",
    "@tacobot/gigatacos-client": "workspace:*",
    "axios": "^1.13.2",
    "better-auth": "^1.3.34",
    "bs58": "^6.0.0",
    "cheerio": "^1.1.2",
    "date-fns": "^4.1.0",
    "dotenv": "^17.2.3",
    "hono": "^4.10.4",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^6.19.0",
    "reflect-metadata": "^0.2.2",
    "tslib": "^2.8.1",
    "tsyringe": "^4.10.0",
    "uuid": "^13.0.0",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^5.0.0",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@types/bs58": "^5.0.0",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.0",
    "@types/uuid": "^11.0.0",
    "@vitest/coverage-v8": "^4.0.7",
    "@vitest/ui": "^4.0.7",
    "esbuild": "^0.25.12",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3",
    "vitest": "^4.0.7"
  },
  "engines": {
    "node": ">=18.0.0",
    "npm": ">=9.0.0"
  }
}

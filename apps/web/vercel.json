{
  "$schema": "https://openapi.vercel.sh/vercel.json",
  "buildCommand": "bun install && bun turbo build --filter=@tacobot/web",
  "framework": "vite",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://tacocrew-api.vercel.app/api/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}

{
  "name": "tacocrew",
  "private": true,
  "version": "0.2512.0",
  "type": "module",
  "packageManager": "pnpm@10.22.0",
  "devDependencies": {
    "@biomejs/biome": "2.3.6",
    "esbuild": "^0.27.0",
    "storybook": "10.0.8",
    "turbo": "^2.5.5"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:api": "pnpm --filter @tacobot/api dev",
    "dev:web": "pnpm --filter @tacobot/web dev",
    "dev:storybook": "pnpm --filter @tacobot/storybook dev",
    "build": "turbo run build",
    "build:ui-kit": "pnpm --filter @tacobot/ui-kit build",
    "db:setup": "pnpm --filter @tacobot/api db:setup",
    "test": "turbo run test",
    "check": "pnpm --filter @tacobot/api check"
  },
  "dependencies": {
    "@radix-ui/react-tooltip": "^1.2.8"
  }
}

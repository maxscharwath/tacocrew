{
  "name": "tacocrew",
  "private": true,
  "version": "0.2512.2",
  "type": "module",
  "packageManager": "bun@1.2.14",
  "workspaces": ["apps/*", "packages/*"],
  "devDependencies": {
    "@biomejs/biome": "2.3.9",
    "glob": "^13.0.0",
    "storybook": "10.1.9",
    "turbo": "^2.6.3",
    "@types/bun": "latest"
  },
  "scripts": {
    "dev": "turbo run dev --parallel",
    "dev:api": "bun --filter @tacocrew/api dev",
    "dev:web": "bun --filter @tacocrew/web dev",
    "dev:storybook": "bun --filter @tacocrew/storybook dev",
    "build": "turbo run build",
    "build:ui-kit": "bun --filter @tacocrew/ui-kit build",
    "db:setup": "bun --filter @tacocrew/api db:setup",
    "test": "turbo run test",
    "check": "turbo run check",
    "lint:fix": "biome check --write .",
    "check:translations": "bun scripts/check-translations.ts"
  },
  "module": "index.ts",
  "peerDependencies": {
    "typescript": "^5.9.3"
  }
}

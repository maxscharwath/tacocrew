{
  "display_information": {
    "name": "TacoBot",
    "description": "Authentication and ordering system for TacoBot",
    "background_color": "#6366f1"
  },
  "features": {
    "bot_user": {
      "display_name": "TacoBot",
      "always_online": false
    }
  },
  "oauth_config": {
    "redirect_urls": [
      "http://localhost:4000/api/auth/callback/slack",
      "https://yourdomain.com/api/auth/callback/slack"
    ],
    "scopes": {
      "user": ["openid", "profile", "email"]
    }
  },
  "settings": {
    "interactivity": {
      "is_enabled": false
    },
    "org_deploy_enabled": false,
    "socket_mode_enabled": false,
    "token_rotation_enabled": false
  }
}
